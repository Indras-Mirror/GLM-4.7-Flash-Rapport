{
  "mcpServers": {
    "googlesearch": {
      "command": "node",
      "args": [
        "REPLACE_WITH_FULL_PATH/lib/mcp-servers/googlesearch/mcp-server/index.js"
      ],
      "env": {
        "GOOGLE_SEARCH_API_KEY": "your-google-api-key-here",
        "GOOGLE_SEARCH_CX": "your-custom-search-engine-id"
      }
    },
    "visionproxy": {
      "command": "node",
      "args": [
        "REPLACE_WITH_FULL_PATH/lib/mcp-servers/visionproxy/mcp-server/index.js"
      ],
      "env": {
        "OPENROUTER_API_KEY": "your-openrouter-api-key-here"
      }
    }
  }
}
